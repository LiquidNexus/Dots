set number
set relativenumber
syntax on
colorscheme ron
set tabstop=4
set shiftwidth=4
set expandtab
set softtabstop=4
set autoindent
filetype indent on
filetype detect
set foldmethod=indent
set foldlevel=20
set wrap linebreak nolist
nnoremap <Down> gj
nnoremap <Up> gk
let g:powerline_pycmd="py3"
set laststatus=2
